{
  "name": "lastfm-app",
  "version": "0.0.1",
  "description": "A single page app that displays stats pulled from the Last.fm API.",
  "devEngines": {
    "node": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slwen/lastfm-app.git"
  },
  "dependencies": {
    "color": "^0.7.3",
    "jsonp": "^0.1.0",
    "lodash": "^3.10.1",
    "moment": "^2.8.4",
    "numeral": "^1.5.3",
    "paths-js": "^0.3.0",
    "react": "^0.13.3",
    "react-motion": "^0.3.1",
    "react-router": "^0.13.3",
    "reflux": "^0.2.12"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.9.0",
    "eslint": "^1.7.1",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "file-loader": "^0.8.1",
    "http-server": "^0.7.4",
    "jest-cli": "^0.2.1",
    "json-loader": "^0.5.1",
    "jsx-loader": "^0.12.2",
    "node-sass": "^3.3.2",
    "react-tools": "^0.12.2",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.8.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.7.0"
  },
  "scripts": {
    "serve": "./node_modules/.bin/http-server -p 8080",
    "dev": "webpack-dev-server --progress --colors --port 8090",
    "start": "npm run serve | npm run dev",
    "build": "webpack -p",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor",
    "unmockedModulePathPatterns": [
      "react"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testFileExtensions": [
      "js"
    ]
  },
  "author": "Sam Enoka"
}
